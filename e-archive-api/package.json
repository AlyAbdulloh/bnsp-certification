  "scripts": {
    "dev": "nodemon -r dotenv/config app.js dotenv_config_path=./env/.env.dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "models": "node ./config/sequelizeAuto.js",
    "models-dev": "node ./config/sequelizeAuto.js --env=../env/.env.dev",
    "create-migration": "node ./config/migration.js",
    "migrate:dev": "cross-env --env=../env/.env.dev npx sequelize-cli db:migrate --env example --migrations-path ./src/migrations",
    "migrate:prod": "cross-env --env=../env/.env.prod npx sequelize-cli db:migrate --env example --migrations-path ./src/migrations",
    "migrate-undo:dev": "cross-env --env=../env/.env.dev npx sequelize-cli db:migrate:undo --env example --migrations-path ./src/migrations",
    "migrate-undo:prod": "cross-env --env=../env/.env.prod npx sequelize-cli db:migrate:undo --env example --migrations-path ./src/migrations",
    "migrate-undo:all:dev": "cross-env --env=../env/.env.dev npx sequelize-cli db:migrate:undo:all --env example --migrations-path ./src/migrations",
    "migrate-undo:all:prod": "cross-env --env=../env/.env.prod npx sequelize-cli db:migrate:undo:all --env example --migrations-path ./src/migrations"
  },
